@using EntityLayer.Concrete
@model List<About>

@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/_AdminLayout.cshtml";
}

<h2>Hakkında Veri Listesi </h2>
<table class="table table-bordered">
    <tr>
        <th>ID</th>
        <th>Hakkımda Başlığı</th>
        <th>Aktif Yap</th>
        <th>Pasif Yap</th>
    </tr>

    @foreach (var item in Model)
    {
        <tr>
            <th>@item.AboutID</th>
            <th>Başlık</th>
            <th><a href="#" class="btn btn-warning">Aktif Yap</a></th>
            <th><a href="#" class="btn btn-danger">Pasif Yap</a></th>
        </tr>
    }

</table>




@Html.Action("AboutPartial","About")
